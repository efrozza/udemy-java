
public class Hello {

	public static void main(String[] args) {
		
		// em java usamos int e double como padrao
		
		int value1 = 9;
		
		// float
		// 32 bits
		
		float value2 = 9f;
		
		// double
		// 64 bits 
		
		double value3 = 9d;
		
		System.out.println("value 1 = " + value1);
		System.out.println("value 2 = " + value2);
		System.out.println("value 3 = " + value3);
		

	}

}
