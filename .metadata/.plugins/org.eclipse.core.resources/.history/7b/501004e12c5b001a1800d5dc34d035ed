
public class Hello {
	public static void main(String[] args) {
		// declaração da variavel
		int value1;
		// valorização da variavel
		value1 = 1;
		
		// declaração com inicialização
		int value2 = (2 * 2) - 1;
		int value3 = value2 - value1
		
		System.out.println(value1);
		System.out.println(value2);
	}

}
